library(tidyverse)
data <- read_csv("~/Desktop/BBOXX/sample_2_weeks.csv")
data$power_in <- data$current_in * data$voltage
data$power_out <- data$current_out * data$voltage
power_data <- data %>% select(time, power_in, power_out)
ggplot(power_data) +
geom_point(aes(x=time, y=power_in, colour="Solar")) +
geom_point(aes(x=time, y=power_out, colour="Load")) +
ylab("Power (W)")
data <- data %>%
gather(Variable, measurement, voltage:power_out, factor_key=TRUE)
power_data <- data %>% select(time, power_in, power_out)
pow_plt <- ggplot(power_data) +
geom_point(aes(x=time, y=power_in, colour="Solar")) +
geom_point(aes(x=time, y=power_out, colour="Load")) +
ylab("Power (W)")
ggsave(plot=pow_plt,"~/Desktop/BBOXX/Power.pdf", width=6, height=3.5, units="in")
power_data
power_data <- power_data %>% filter(time>"2018-06-05 02:00:48")
power_data
